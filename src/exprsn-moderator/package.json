{
  "name": "@exprsn/moderator",
  "version": "1.0.0",
  "description": "Content Moderation & Safety Platform",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "worker:queue": "nodemon workers/queue-worker.js",
    "start": "node src/index.js",
    "setup-db": "node scripts/setup-db.js",
    "init": "node scripts/init-system.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "moderation",
    "content-safety",
    "ai",
    "ml",
    "toxicity-detection",
    "certificate-authority",
    "exprsn"
  ],
  "author": "Rick Holland <engineering@exprsn.com>",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.0",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "node-forge": "^1.3.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.24.1",
    "pg": "^8.11.3",
    "rate-limit-redis": "^4.2.0",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.6.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
