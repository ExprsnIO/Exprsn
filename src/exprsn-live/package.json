{
  "name": "@exprsn/live",
  "version": "1.0.0",
  "description": "Live Streaming & Video Chat Platform via Cloudflare",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:load": "k6 run tests/load/streaming.js",
    "migrate": "node scripts/migrate.js",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "live-streaming",
    "video-chat",
    "webrtc",
    "cloudflare",
    "broadcasting",
    "certificate-authority",
    "exprsn"
  ],
  "author": "Rick Holland <engineering@exprsn.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "sequelize": "^6.35.0",
    "redis": "^4.6.11",
    "socket.io": "^4.7.2",
    "ws": "^8.16.0",
    "uuid": "^9.0.1",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "joi": "^17.11.0",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
