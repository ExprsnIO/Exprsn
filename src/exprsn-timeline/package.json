{
  "name": "@exprsn/timeline",
  "version": "1.0.0",
  "description": "Social Feed & Timeline Management System",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "worker": "node src/worker.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:load": "k6 run tests/load/timeline-generation.js",
    "test:ranking": "jest --config jest.ranking.config.js",
    "migrate:pg": "node scripts/migrate-postgres.js",
    "setup:elasticsearch": "node scripts/setup-elasticsearch.js",
    "seed": "node scripts/seed.js",
    "generate-certs": "node scripts/generate-tls-certs.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "social-media",
    "timeline",
    "feed",
    "ranking",
    "real-time",
    "certificate-authority",
    "exprsn"
  ],
  "author": "Rick Holland <engineering@exprsn.com>",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@exprsn/shared": "file:../shared",
    "axios": "^1.6.5",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "emoji-regex": "^10.3.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "linkify-it": "^5.0.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.3",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "sanitize-html": "^2.11.0",
    "sequelize": "^6.35.2",
    "sharp": "^0.33.1",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/sanitize-html": "^2.9.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "socket.io-client": "^4.7.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
