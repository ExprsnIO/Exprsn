{
  "name": "@exprsn/bluesky",
  "version": "1.0.0",
  "description": "Bluesky AT Protocol Personal Data Server integration for Exprsn",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --coverage",
    "test:unit": "jest --testPathIgnorePatterns=integration --coverage",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:api": "jest --testPathPattern=api-coverage --runInBand",
    "test:watch": "jest --watch",
    "test:all": "jest --coverage --runInBand",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "bluesky",
    "atproto",
    "pds",
    "federation",
    "social"
  ],
  "author": "Rick Holland <engineering@exprsn.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@atproto/api": "^0.12.0",
    "@atproto/common-web": "^0.3.0",
    "@atproto/crypto": "^0.4.0",
    "@atproto/identity": "^0.4.0",
    "@atproto/repo": "^0.4.0",
    "@atproto/xrpc-server": "^0.5.0",
    "@exprsn/shared": "file:../shared",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multiformats": "^13.0.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.12",
    "sequelize": "^6.35.2",
    "socket.io": "^4.6.1",
    "uint8arrays": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
