{
  "lexicon": 1,
  "id": "io.exprsn.timeline.repost",
  "defs": {
    "main": {
      "type": "record",
      "description": "Repost record for timeline posts",
      "key": "tid",
      "record": {
        "type": "object",
        "required": ["subject", "createdAt"],
        "properties": {
          "subject": {
            "type": "ref",
            "ref": "com.atproto.repo.strongRef",
            "description": "Reference to the reposted post"
          },
          "createdAt": {
            "type": "string",
            "format": "datetime"
          }
        }
      }
    }
  }
}
