{
  "name": "@exprsn/dbadmin",
  "version": "1.0.0",
  "description": "PostgreSQL Database Administration & Management Interface",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "node scripts/seed.js",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "postgresql",
    "database-admin",
    "pgadmin",
    "database-management",
    "sql-editor",
    "exprsn"
  ],
  "author": "Rick Holland <engineering@exprsn.com>",
  "license": "MIT",
  "dependencies": {
    "@exprsn/shared": "file:../shared",
    "axios": "^1.6.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.6",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonata": "^2.1.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "pg-format": "^1.0.4",
    "sequelize": "^6.35.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "socket.io-client": "^4.7.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
