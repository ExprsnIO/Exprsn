{
  "name": "@exprsn/gallery",
  "version": "1.0.0",
  "description": "Versioned and Tokenized Media Galleries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "worker": "node src/workers/media.js",
    "worker:dev": "nodemon src/workers/media.js",
    "build": "echo 'Build step (if needed)'",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "migrate": "node scripts/migrate.js",
    "lint": "eslint src --ext .js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "gallery",
    "photos",
    "videos",
    "albums",
    "media-processing",
    "certificate-authority",
    "exprsn"
  ],
  "author": "Rick Holland <engineering@exprsn.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "exif-parser": "^0.1.12",
    "fluent-ffmpeg": "^2.1.2",
    "uuid": "^9.0.1",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "bull": "^4.12.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/multer": "^1.4.11",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
